elapsedMicros afterOneLoop;
elapsedMicros runTime;


int delayTime;
int totalTime;

int pos;
int rad;

void oneTurnTime() {
  start = true;
  delayTime = (runTime - totalTime)/41;
}
 
void setup() {
  pinMode(intTriggerPin, INPUT);
  attachInterrupt(intTriggerPin, oneTurnTime, RISING);

  strip.begin();
  strip.setBrightness(100);

  int beforePaint = afterOneLoop;
  paint(0);
  totalTime = afterOneLoop - beforePaint;
  delayTime = (runTime - totalTime)/41;
}
 
void loop() {
  while(!start){}
  start = false;
  runTime = 0;
  paint(constrain(delayTime, 0, 150000));

}

void paint(int del) {
  for(int d=0; d<40; d++){
      for(int i=0; i<4; i++){
        int iMten = (i*10);
        rad = (d + iMten)%40;   
        for(int j=0; j< 10; j++){
            pos = i+(j*spokes);
            strip.setPixelColor(j + iMten, pixel_grid_red[rad][pos], pixel_grid_green[rad][pos], pixel_grid_blue[rad][pos]);
        }
    }
    strip.show();
    if(start) {
      return;
    }
    delayMicroseconds(del);
  }
  for(int i=0; i<60; i++){
      strip.setPixelColor(i,strip.Color(0,0,0));
  }
  strip.show();
}
